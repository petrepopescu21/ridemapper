{
    "name": "ridemapper-server",
    "version": "1.0.0",
    "description": "RideMapper WebSocket server for real-time location tracking and route management",
    "main": "dist/index.js",
    "scripts": {
        "dev": "nodemon --exec ts-node src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "clean": "rimraf dist",
        "type-check": "tsc --noEmit"
    },
    "dependencies": {
        "concurrently": "^9.1.2",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^4.18.2",
        "socket.io": "^4.7.4",
        "socket.io-client": "^4.8.1",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.5",
        "@types/uuid": "^9.0.7",
        "nodemon": "^3.0.2",
        "rimraf": "^5.0.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "keywords": [
        "websocket",
        "real-time",
        "location-tracking",
        "route-management"
    ],
    "author": "RideMapper Team",
    "license": "MIT"
}
